<agent xmlns="http://jadex.sourceforge.net/jadex"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://jadex.sourceforge.net/jadex
	                    http://jadex.sourceforge.net/jadex-0.96.xsd"
       name="Estanteria"
       package="dormitorio.estanteria">
    <imports>
        <import>jadex.planlib.*</import>
    </imports>
    <capabilities>
        <capability name="dfcap" file="jadex.planlib.DF"/>
    </capabilities>
    <goals>
        <maintaingoalref name="df_diversion_leer_libro">
            <concrete ref="dfcap.df_diversion_leer_libro"/>
        </maintaingoalref>
        <maintaingoalref name="df_carisma_leer_libro">
            <concrete ref="dfcap.df_carisma_leer_libro"/>
        </maintaingoalref>
        <maintaingoalref name="df_cocina_leer_libro">
            <concrete ref="dfcap.df_cocina_leer_libro"/>
        </maintaingoalref>
        <maintaingoalref name="df_jardineria_leer_libro">
            <concrete ref="dfcap.df_jardineria_leer_libro"/>
        </maintaingoalref>
        <maintaingoalref name="df_pintura_leer_libro">
            <concrete ref="dfcap.df_pintura_leer_libro"/>
        </maintaingoalref>
        <maintaingoalref name="df_fotografia_leer_libro">
            <concrete ref="dfcap.df_fotografia_leer_libro"/>
        </maintaingoalref>
        <maintaingoalref name="df_logica_leer_libro">
            <concrete ref="dfcap.df_logica_leer_libro"/>
        </maintaingoalref>
        <maintaingoalref name="df_ciencia_leer_libro">
            <concrete ref="dfcap.df_ciencia_leer_libro"/>
        </maintaingoalref>
    </goals>
    <plans>
        <plan name="LeerLibroPlan">
            <body class="dormitorio.estanteria.LeerLibroPlan"/>
            <trigger>
                <messageevent ref="request_leer_libro"/>
            </trigger>
        </plan>
    </plans>
    <events>
        <messageevent name="request_leer_libro" direction="receive" type="fipa">
            <parameter name="performative" class="String" direction="fixed">
                <value>jadex.adapter.fipa.SFipa.REQUEST</value>
            </parameter>
            <parameter name="content" class="ontologia.acciones.LeerLibro"/>
        </messageevent>
        <messageevent name="libro_leido" direction="send" type="fipa">
            <parameter name="performative" class="String" direction="fixed">
                <value>jadex.adapter.fipa.SFipa.INFORM_REF</value>
            </parameter>
            <parameter name="content" class="ontologia.predicados.LibroLeido"/>

        </messageevent>
        <messageevent name="libro_terminado" direction="send" type="fipa">
            <parameter name="performative" class="String" direction="fixed">
                <value>jadex.adapter.fipa.SFipa.INFORM_REF</value>
            </parameter>
            <parameter name="content" class="ontologia.predicados.LibroTerminado"/>
        </messageevent>
    </events>
    <configurations>
        <configuration name="default">
            <goals>
                <initialgoal ref="df_diversion_leer_libro">
                    <parameter ref="description">
                        <value>
                            SFipa.createAgentDescription(null,
                            SFipa.createServiceDescription("Diversion", "LeerLibro"))
                        </value>
                    </parameter>
                </initialgoal>
                <initialgoal ref="df_carisma_leer_libro">
                    <parameter ref="description">
                        <value>
                            SFipa.createAgentDescription(null,
                            SFipa.createServiceDescription("Carisma", "LeerLibro"))
                        </value>
                    </parameter>
                </initialgoal>
                <initialgoal ref="df_cocina_leer_libro">
                    <parameter ref="description">
                        <value>
                            SFipa.createAgentDescription(null,
                            SFipa.createServiceDescription("Cocina", "LeerLibro"))
                        </value>
                    </parameter>
                </initialgoal>
                <initialgoal ref="df_jardineria_leer_libro">
                    <parameter ref="description">
                        <value>
                            SFipa.createAgentDescription(null,
                            SFipa.createServiceDescription("Jardineria", "LeerLibro"))
                        </value>
                    </parameter>
                </initialgoal>
                <initialgoal ref="df_pintura_leer_libro">
                    <parameter ref="description">
                        <value>
                            SFipa.createAgentDescription(null,
                            SFipa.createServiceDescription("Pintura", "LeerLibro"))
                        </value>
                    </parameter>
                </initialgoal>
                <initialgoal ref="df_fotografia_leer_libro">
                    <parameter ref="description">
                        <value>
                            SFipa.createAgentDescription(null,
                            SFipa.createServiceDescription("Fotografia", "LeerLibro"))
                        </value>
                    </parameter>
                </initialgoal>
                <initialgoal ref="df_logica_leer_libro">
                    <parameter ref="description">
                        <value>
                            SFipa.createAgentDescription(null,
                            SFipa.createServiceDescription("Logica", "LeerLibro"))
                        </value>
                    </parameter>
                </initialgoal>
                <initialgoal ref="df_ciencia_leer_libro">
                    <parameter ref="description">
                        <value>
                            SFipa.createAgentDescription(null,
                            SFipa.createServiceDescription("Ciencia", "LeerLibro"))
                        </value>
                    </parameter>
                </initialgoal>
                <endgoal ref="df_deregister"/>
            </goals>
        </configuration>
    </configurations>
</agent>